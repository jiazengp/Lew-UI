{
  "name": "lew-ui",
  "version": "2.7.3",
  "type": "module",
  "description": "A Component Library for Vue3.js.",
  "keywords": [
    "vue3",
    "components",
    "ui library",
    "frontend"
  ],
  "author": "lewkamtao",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:tsdown": "tsdown",
    "build:docs": "vue-tsc --noEmit && vite build --mode docs",
    "build:lib": "vue-tsc --noEmit && vite build --mode lib",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --cache --write ."
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    },
    "./style": "./dist/style.css"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@types/lodash-es": "^4.17.12",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.6.7",
    "dayjs": "^1.11.7",
    "highlight.js": "^11.9.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.471.0",
    "rollup-plugin-scss": "^4.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "tippy.js": "^6.3.7",
    "vue-i18n": "^11.1.0",
    "vue-router": "^4.0.13",
    "vue-virt-list": "^1.5.11",
    "vuedraggable": "^4.1.0",
    "yup": "^1.2.0"
  },
  "peerDependencies": {
    "vue": "^3.5.3"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.0.4",
    "@sxzz/prettier-config": "^2.2.3",
    "@types/node": "^22.16.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^10.2.0",
    "eslint": "^9.30.1",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "terser": "^5.31.6",
    "tsdown": "^0.12.9",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^7.0.2",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-zip-pack": "^1.2.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-tsc": "^3.0.1"
  },
  "prettier": "@sxzz/prettier-config"
}
