{
  "name": "lew-ui",
  "type": "module",
  "version": "2.7.6",
  "description": "A Component Library for Vue3.js.",
  "author": "lewkamtao",
  "license": "MIT",
  "keywords": [
    "vue3",
    "components",
    "ui library",
    "frontend"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style": "./dist/index.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "build:tsdown": "tsdown",
    "build:docs": "vue-tsc --noEmit && vite build --mode docs",
    "build:lib": "vue-tsc --noEmit && vite build --mode lib",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.5.3"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@types/lodash-es": "^4.17.12",
    "@vueuse/core": "^13.5.0",
    "axios": "^1.6.7",
    "dayjs": "^1.11.7",
    "highlight.js": "^11.9.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.471.0",
    "rollup-plugin-scss": "^4.0.1",
    "tippy.js": "^6.3.7",
    "vue-i18n": "^11.1.0",
    "vue-router": "^4.0.13",
    "vue-virt-list": "^1.5.11",
    "vuedraggable": "^4.1.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@types/node": "^22.16.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "sass": "^1.89.2",
    "tsdown": "^0.12.9",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^7.0.2",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-zip-pack": "^1.2.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-tsc": "^3.0.1"
  }
}
